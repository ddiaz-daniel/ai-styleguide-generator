<!DOCTYPE html>
<html lang="{{ lang }}">
    <head>
        <meta charset="utf-8"/>

        <title>{{ meta_title }}</title>
        <meta name="description" content="{{ meta_description }}">

        {% if lang == "en" %}
            <meta property="og:locale" content="en_EN" />
        {% else  %}
            <meta property="og:locale" content="de_DE" />
        {% endif %}

        <meta property="og:type" content="website" />
        <meta property="og:title" content="AI Styleguide Generator" />

        {% if lang == "en" %}
            <meta property="og:description" content="Create your style guide in minutes" />
        {% else %}
            <meta property="og:description" content="Erstellen Sie Ihren Online-Styleguide in wenigen Minuten" />
        {% endif %}
    

        <meta name="theme-color" content="#ffffff">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        {% if noindex == true %}
            <meta name="robots" content="noindex" />
        {% endif %}

        <script
  src="https://code.jquery.com/jquery-3.7.0.min.js"
  integrity="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g="
  crossorigin="anonymous"></script>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css">
        <script src="https://npmcdn.com/flatpickr/dist/flatpickr.min.js"></script>
        <script src="https://npmcdn.com/flatpickr/dist/l10n/de.js"></script>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <link rel="stylesheet" href="application/assets/style/transpiler.php/main.scss?v=38">

        <!--script src="application/assets/script/main.js?v=33"></script-->
        <script src="https://kit.fontawesome.com/d7e8014efc.js" crossorigin="anonymous"></script>


    </head>
</html>


{% block body %}
	<header>
		{% block header %}{% endblock %}
	</header>
	<main>
		{% block content %}{% endblock %}
	</main>
	<footer>
		{% block footer %}{% endblock %}
	</footer>
    {% block overlay %}{% endblock %}
{% endblock %}
